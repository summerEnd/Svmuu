<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.svmuu.slibdemo" >

    <application
        android:name="com.slib.demo.AppDelegate"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.slib.demo.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.slib.demo.PagerTitle"
            android:label="@string/title_activity_pager_title" />
        <activity
            android:name="com.slib.demo.HttpTest"
            android:label="@string/title_activity_pager_title" />
        <activity android:name="com.sp.lib.activity.DEBUGActivity" />
        <activity android:name="com.sp.lib.activity.album.PhotoAlbumActivity" />
        <activity android:name="com.slib.demo.AnimTest" />
        <activity
            android:name="com.slib.demo.AutoLayoutTest"
            android:label="@string/title_activity_auto_layout_test" />
        <activity
            android:name="com.slib.demo.JiHuanSongTest"
            android:label="@string/title_activity_ji_huan_song_test" />
        <activity android:name="com.slib.demo.WaveTest" />
        <activity android:name="com.slib.demo.DreamTest" />
        <activity android:name="com.slib.demo.ToggleTest" />
        <activity android:name="com.slib.demo.WidgetHost" />
        <activity android:name="com.slib.demo.material.MaterialBaseTest" />
        <activity android:name="com.slib.demo.material.MaterialTest" />
        <activity android:name="com.slib.demo.material.MaterialGridView" />

        <receiver android:name="com.slib.demo.WidgetHost$MyWidget" >
            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE" />
            </intent-filter>

            <meta-data
                android:name="android.appwidget.provider"
                android:resource="@xml/provider" />
        </receiver>

        <activity
            android:name="com.slib.demo.LockTest"
            android:label="@string/title_activity_lock_test" >
        </activity>
    </application>

</manifest>
