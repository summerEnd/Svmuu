<resources>

    <style name="mainMenu">

    </style>

    <style name="mainMenu.clickItem">
        <item name="android:background">@drawable/main_menu_click_item</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingTop">4dp</item>
        <item name="android:paddingBottom">4dp</item>
        <item name="android:layout_marginBottom">1px</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="mainMenuClickItemTextStyle">
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@drawable/s_text_white_gray</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="mainMenuClickItemIconStyle">
        <item name="android:scaleType">centerInside</item>
        <item name="android:layout_width">@dimen/menuIconSize</item>
        <item name="android:layout_height">@dimen/menuIconSize</item>
    </style>

    <style name="horizontalSpace">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="horizontalLine">
        <item name="android:layout_height">1px</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">#d5d5d5</item>
    </style>

    <style name="horizontalLine.homeMenu">
        <item name="android:background">#3B3B43</item>
    </style>

    <style name="verticalLine" parent="horizontalLine">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">1px</item>
    </style>

    <style name="loginDialogEditTextStyle">
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:paddingLeft">@dimen/loginDialog_edit_padding_horizontal</item>
        <item name="android:paddingRight">@dimen/loginDialog_edit_padding_horizontal</item>
        <item name="android:layout_height">@dimen/login_dialog_editText_height</item>
        <item name="android:textSize">@dimen/text_26px</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="blueButton">
        <item name="android:background">@drawable/blue_button</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="blueButton.big">
        <item name="android:textSize">@dimen/text_30px</item>
    </style>

    <style name="blueButton.mid">
        <item name="android:textSize">@dimen/text_26px</item>
        <item name="android:layout_height">@dimen/mid_button_height</item>
        <item name="android:layout_width">@dimen/mid_button_width</item>
    </style>

    <style name="redButton">
        <item name="android:background">@drawable/red_button</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="redButton.big">
        <item name="android:textSize">@dimen/text_30px</item>
    </style>

    <style name="redButton.mid">
        <item name="android:textSize">@dimen/text_26px</item>
    </style>

    <style name="tabStyle">
        <item name="android:textSize">13sp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="homeClickItem">
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="homeClickItem.text">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:clickable">false</item>
    </style>

    <style name="homeTextViewStyle1">
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:textSize">@dimen/text_22px</item>
        <item name="android:textColor">@color/textColorBlack</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="capitalFragmentLayoutStyle_1">
        <item name="android:background">@color/lightGray</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:padding">5dp</item>
    </style>

    <style name="TwoTextItem" parent="android:TextAppearance">
        <item name="android:textSize">@dimen/text_22px</item>
        <item name="android:text">test</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="TwoTextItem.Red">
        <item name="android:textColor">@color/textColorRed</item>
    </style>

    <style name="TwoTextItem.Green">
        <item name="android:textColor">@color/textColorGreen</item>
    </style>

    <style name="centerPopAnimation" parent="popup_window_animation">
        <item name="android:windowEnterAnimation">@anim/center_pop_enter</item>
        <item name="android:windowExitAnimation">@anim/center_pop_exit</item>
    </style>

    <style name="applyFragmentTextStyle">
        <item name="android:textColor">@color/textColorDeepGray</item>
        <item name="android:textSize">@dimen/text_24px</item>
    </style>

    <style name="applyFragmentTextStyle.big">
        <item name="android:textColor">@color/textColorBlack</item>
        <item name="android:textSize">@dimen/text_26px</item>
    </style>

    <style name="applyFragmentItemPadding">
        <item name="android:paddingLeft">9dp</item>
        <item name="android:paddingRight">9dp</item>
        <item name="android:paddingTop">14dp</item>
        <item name="android:paddingBottom">14dp</item>
    </style>

    <style name="activityPadding">
        <item name="android:paddingLeft">@dimen/activity_horizontal_margin</item>
        <item name="android:paddingRight">@dimen/activity_horizontal_margin</item>
        <item name="android:paddingTop">@dimen/activity_vertical_margin</item>
        <item name="android:paddingBottom">@dimen/activity_vertical_margin</item>
    </style>

    <style name="applyActivityTextStyle1">
        <item name="android:textColor">@color/textColorDeepGray</item>
        <item name="android:textSize">@dimen/text_28px</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="applyActivityTextStyle2">
        <item name="android:textColor">@color/textColorBlack</item>
        <item name="android:textSize">@dimen/text_30px</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="applyActivityTextStyle2.yuan">
        <item name="android:textSize">@dimen/text_26px</item>
    </style>

    <style name="circleStyle">
        <item name="android:layout_width">@dimen/circleItemSize</item>
        <item name="android:layout_height">@dimen/circleItemSize</item>
    </style>

    <style name="horizontalMargin">
        <item name="android:layout_marginLeft">@dimen/activity_horizontal_margin</item>
        <item name="android:layout_marginRight">@dimen/activity_horizontal_margin</item>
    </style>

    <style name="verticalMargin">
        <item name="android:layout_marginTop">@dimen/activity_vertical_margin</item>
        <item name="android:layout_marginBottom">@dimen/activity_vertical_margin</item>
    </style>

    <style name="homeLoginedText">
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">@dimen/text_28px</item>
        <item name="android:drawablePadding">5dp</item>
        <item name="android:textColor">@color/textColorBlack</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="changeDataTabRow">
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:paddingRight">8dp</item>
        <item name="android:paddingTop">12dp</item>
        <item name="android:paddingBottom">12dp</item>
        <item name="android:background">@color/white</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="changeDataTabName">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/text_28px</item>
        <item name="android:textColor">@color/textColorBlack</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="changeDataTabValue">
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_toLeftOf">@id/arrow</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/text_26px</item>
        <item name="android:textColor">@color/textColorDeepGray</item>
    </style>

    <style name="changeDataTabImage">
        <item name="android:id">@id/arrow</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@drawable/ic_right_arrow</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>
</resources>
