<resources>

    <style name="mainMenu">

    </style>

    <style name="wrapLayout">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="mainMenu.clickItem">
        <item name="android:background">@drawable/main_menu_click_item</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">@dimen/dimen_36px</item>
        <item name="android:paddingRight">@dimen/dimen_36px</item>
        <item name="android:paddingTop">4dp</item>
        <item name="android:paddingBottom">4dp</item>
        <item name="android:layout_marginBottom">1px</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="mainMenuClickItemTextStyle">
        <item name="android:duplicateParentState">true</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_marginLeft">@dimen/dimen_32px</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@drawable/s_text_white_gray</item>
        <item name="android:textSize">@dimen/text_24px</item>
        <item name="android:drawableRight">@drawable/left_menu_arrow</item>
    </style>

    <style name="mainMenuClickItemIconStyle">
        <item name="android:duplicateParentState">true</item>
        <item name="android:scaleType">centerInside</item>
        <item name="android:layout_width">@dimen/menuIconSize</item>
        <item name="android:layout_height">@dimen/menuIconSize</item>
    </style>

    <style name="horizontalSpace">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="horizontalLine">
        <item name="android:layout_height">1px</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">#d5d5d5</item>
    </style>

    <style name="horizontalLine.homeMenu">
        <item name="android:background">#3B3B43</item>
    </style>

    <style name="verticalLine" parent="horizontalLine">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">1px</item>
    </style>

    <style name="dialogPadding">
        <item name="android:paddingTop">@dimen/dimen_20px</item>
        <item name="android:paddingRight">@dimen/dimen_20px</item>
        <item name="android:paddingLeft">@dimen/dimen_20px</item>
        <item name="android:paddingBottom">@dimen/dimen_40px</item>
    </style>

    <style name="loginDialogEditTextStyle">
        <item name="android:paddingLeft">@dimen/loginDialog_edit_padding_horizontal</item>
        <item name="android:paddingRight">@dimen/loginDialog_edit_padding_horizontal</item>
        <item name="android:layout_height">@dimen/dimen_93px</item>
        <item name="android:textSize">@dimen/text_24px</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="blueButton" parent="wrapLayout">
        <item name="android:background">@drawable/blue_button</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="blueButton.big">
        <item name="android:textSize">@dimen/text_28px</item>
