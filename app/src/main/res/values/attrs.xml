<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="textSize" format="dimension" />

    <declare-styleable name="TabItem">
        <attr name="checked" format="boolean" />
        <attr name="tabDrawable" format="reference" />
        <attr name="tabTextColor" format="reference" />
    </declare-styleable>
    <declare-styleable name="TwoTextItem">

        <attr name="prefix" format="string" />
        <attr name="prefixStyle" format="reference" />
        <attr name="prefixTextColor" format="color" />
        <attr name="prefixTextSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CircleItem">
        <attr name="boldText" format="string" />
        <attr name="normalText" format="string" />
        <attr name="circle" format="enum">
            <enum name="red" value="1" />
            <enum name="blue" value="0" />
        </attr>
    </declare-styleable>
</resources>